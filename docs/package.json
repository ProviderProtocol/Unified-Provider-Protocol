{
  "name": "docs",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "build:clean": "rm -rf .astro node_modules/.astro && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "clean": "rm -rf .astro node_modules/.astro dist",
    "clean:ai": "rm -rf src/content/docs/ai && mkdir -p src/content/docs/ai",
    "clean:agents": "rm -rf src/content/docs/agents && mkdir -p src/content/docs/agents",
    "spec:generate": "bun run ../scripts/generate-spec-docs.ts",
    "spec:generate:upp": "bun run ../scripts/generate-spec-docs.ts upp",
    "spec:generate:uap": "bun run ../scripts/generate-spec-docs.ts uap",
    "spec:deploy": "bun run spec:generate && rm -rf .astro node_modules/.astro"
  },
  "dependencies": {
    "@astrojs/starlight": "^0.37.1",
    "astro": "^5.6.1",
    "sharp": "^0.34.2",
    "starlight-page-actions": "^0.3.0"
  }
}
